{
    "name": "letters-social",
    "version": "1.0.0",
    "description": "Sample application for React in Action",
    "main": "./src/index.js",
    "@std/esm": {
        "esm": "all",
        "cjs": true
    },
    "now": {
        "name": "letters-social",
        "alias": "social.react.sh",
        "dotenv": ".env.production"
    },
    "scripts": {
        "analyze": "cross-env ANALYZE=true npm run build",
        "api": "nodemon -x node -r @std/esm ./db/run.js",
        "build:app": "cross-env NODE_ENV=production webpack -p --progress --config ./build/webpack.config.prod.js",
        "build": "cross-env NODE_ENV=production npm-run-all build:app db:seed",
        "client:dev": "webpack-dev-server --config ./build/webpack.config.js",
        "doc": "esdoc -c esdoc.json",
        "db:seed": "node -r @std/esm db/seed.js",
        "deploy": "now -e LETTERS_FIREBASE_ADMIN_KEY=${LETTERS_FIREBASE_ADMIN_KEY} -d",
        "dev": "cross-env NODE_ENV=development npm-run-all -p api client:dev server:dev styles:w",
        "server:dev": "nodemon --config nodemon.json index.js",
        "start": "cross-env NODE_ENV=production pm2 start ./index.js --no-daemon",
        "styles:w": "npm run styles && node-sass src/styles/styles.scss -o static/ -w src/styles --watch --recursive",
        "styles": "node-sass -q src/styles/styles.scss -o static/",
        "test:w": "jest --watch --coverage",
        "test": "jest --coverage"
    },
    "engines": {
        "node": "8.9.3",
        "npm": "5.5.1"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFiles": [
            "raf/polyfill",
            "./test/setup.js"
        ],
        "globals": {
            "__DEV__": true,
            "NODE_ENV": "test",
            "ENDPOINT": "http://localhost:3500",
            "GOOGLE_API_KEY": "key",
            "FIREBASE_AUTH_DOMAIN": "url",
            "MAPBOX_API_TOKEN": "token"
        }
    },
    "dependencies": {
        "@std/esm": "^0.25.5",
        "axios": "^1.4.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-register": "6.26.0",
        "bad-words": "^3.0.4",
        "body-parser": "^1.20.2",
        "classnames": "2.3.2",
        "compression": "^1.7.4",
        "config": "^3.3.9",
        "cookie-parser": "1.4.6",
        "cookies": "0.8.0",
        "cors": "2.8.5",
        "enroute": "1.0.1",
        "environs": "0.4.5",
        "express": "^4.18.2",
        "firebase": "^9.22.0",
        "firebase-admin": "^11.8.0",
        "font-awesome": "4.7.0",
        "helmet": "^7.0.0",
        "history": "5.3.0",
        "hpp": "0.2.3",
        "humanize-duration": "^3.28.0",
        "invariant": "2.2.4",
        "isomorphic-fetch": "3.0.0",
        "js-cookie": "^3.0.5",
        "json-server": "^0.17.3",
        "lodash": "^4.17.21",
        "mapbox": "1.0.0-beta9",
        "morgan": "^1.10.0",
        "parse-link-header": "2.0.0",
        "pm2": "^5.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "8.0.5",
        "react-router": "6.11.2",
        "redux": "4.2.1",
        "redux-thunk": "2.4.2",
        "response-time": "2.3.2",
        "serve-favicon": "^2.5.0",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.21.8",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.21.5",
        "autoprefixer": "^10.4.14",
        "babel-eslint": "^8.0.3",
        "babel-jest": "^29.5.0",
        "babel-loader": "9.1.2",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react-app": "^10.0.1",
        "coveralls": "3.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "6.7.4",
        "cssnano": "6.0.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.7",
        "esdoc": "^1.1.0",
        "esdoc-ecmascript-proposal-plugin": "^1.0.0",
        "esdoc-jsx-plugin": "^1.0.0",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react": "^7.32.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "faker": "6.6.6",
        "file-loader": "6.2.0",
        "forcem-ipsum": "2.0.1",
        "html-webpack-plugin": "5.5.1",
        "jest": "^29.5.0",
        "jest-enzyme": "^7.1.2",
        "jsdoc-babel": "^0.5.0",
        "loaders.css": "0.1.2",
        "mkdirp": "3.0.1",
        "node-sass": "^9.0.0",
        "nodemon": "^2.0.22",
        "npm-run-all": "^4.1.5",
        "ora": "6.3.1",
        "postcss-cssnext": "3.0.2",
        "postcss-import": "^15.1.0",
        "postcss-loader": "^7.3.0",
        "prettier": "^2.8.8",
        "raf": "^3.4.1",
        "react-test-renderer": "^18.2.0",
        "redux-mock-store": "1.5.4",
        "regenerator-runtime": "^0.13.11",
        "sass-loader": "13.3.0",
        "starwars": "1.0.1",
        "style-loader": "^3.3.3",
        "sw-precache-webpack-plugin": "1.0.0",
        "url-loader": "^4.1.1",
        "webpack": "^5.83.1",
        "webpack-bundle-analyzer": "^4.8.0",
        "webpack-cli": "^5.1.1",
        "webpack-dev-middleware": "^6.1.1",
        "webpack-dev-server": "^4.15.0",
        "webpack-hot-middleware": "^2.25.3",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-md5-hash": "0.0.6"
    }
}
